<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Agence Immo{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="https://bootswatch.com/5/spacelab/bootstrap.min.css">
    {% endblock %}

    {% block javascripts %}
        {# {{ encore_entry_script_tags('app') }} #}
    {% endblock %}
</head>
<body>
{% block navbar %}
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
        <div class="container-fluid">
            <a class="navbar-brand" href="{{ path('home_browse') }}">Mon Agence</a>
            <div class="collapse navbar-collapse" id="navbarColor03">
                <ul class="navbar-nav me-auto">
                    <li class="nav-item">
                        <a class="nav-link {% if current_page is defined  and current_page == 'home' %} active {% endif %}"
                           href="{{ path('home_browse') }}">Accueil</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link {% if current_page is defined and current_page == 'properties' %} active {% endif %}"
                           href="{{ path('property_browse') }}">Acheter</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
{% endblock %}
{% block body %}
{% endblock %}
</body>
</html>
